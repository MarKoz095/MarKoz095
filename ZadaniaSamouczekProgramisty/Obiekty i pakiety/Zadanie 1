/* Utwórz klasę Human reprezentującą
człowieka, musi posiadać atrybuty takie jak
wiek, waga, wzrost, imię i płeć.
Klasa powinna także zawierać metody getAge, getWeight, getHeight, getName, isMale */


public class Human {

    private final int wiek;
    private final double waga;
    private final int wzrost;
    private final String imie;
    private final char plec;

    public Human(int wiek, int waga, int wzrost, String imie, char plec) {
        this.wiek = wiek;
        this.waga = waga;
        this.wzrost = wzrost;
        this.imie = imie;
        this.plec = plec;
    }

    public int getAge(){
        return wiek;
    }

    public double getWeight(){
        return waga;
    }

    public int getHight(){
        return wzrost;
    }

    public String getName(){
        return imie;
    }
    public char plec(){
        return plec;
    }



}
